cd `dirname $0`
cd ../dstore

codes=( np )

mkdir cache

for code in "${codes[@]}"
do
  if [ ! -f cache/datastore_${code}.xml ]; then
    wget --ignore-length -O cache/datastore_${code}.xml http://datastore.iatistandard.org/api/1/access/activity.xml?recipient-country=${code}\&stream=True || rm -f file
  fi
done

for code in "${codes[@]}"
do
  node js/cmd import "cache/datastore_${code}.xml" || exit 1
done

#make it smarter
node js/cmd vacuum
node js/cmd analyze
